n = 39
remainders = [ ]
while n > 0:
	remainder = n % 2 #remainder of division by 2
	remainders.insert(0, remainder)  # we keep track of
	n //= 2  # we divide n by 2

print (remainders)



people = ['kela' , 'sipa' , 'imut']
ages = [15 , 16 , 17]
position = 0
while position < len(people):
	person = people[position]
	age = ages [position]
	print(person, age)
	position += 1
